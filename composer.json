{
    "name": "ferdinandfrank/vue-forms",
    "description": "A collection of vue.js form and input components to create pretty ajax requests.",
    "license": "MIT",
    "keywords": [
        "vue",
        "forms",
        "inputs",
        "bootstrap",
        "localization",
        "laravel",
        "requests",
        "ajax",
        "language",
        "i18"
    ],
    "authors": [
        {
            "name": "Ferdinand Frank",
            "email": "ferdinand-frank@web.de"
        }
    ],
    "require": {
        "php": ">=7.0",
        "laravel/framework": ">=5.4"
    },
    "autoload": {
        "psr-4": {
            "FerdinandFrank\\VueForms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "FerdinandFrank\\VueForms\\Providers\\VueFormsServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
